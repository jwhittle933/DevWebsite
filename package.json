{
  "name": "devwebsite",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "^5.6.0",
    "grommet-icons": "^4.1.0",
    "path": "^0.12.7",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "node node_modules/eslint/bin/eslint.js 'src/**/*.jsx'",
    "lint-fix": "eslint --fix 'src/**/*.jsx'",
    "pretty": "prettier --list-different './src/**/*.jsx'",
    "pretty-fix": "prettier --write './src/**/*.jsx'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^4.2.4",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.0"
  }
}
